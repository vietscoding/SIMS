@model List<SIMS.Models.Person>


@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
    
*@
@{
    Layout = "_AdminLayout";
}



<h2>People List</h2>
<table class="table">
    <thead>
        <tr>
            <th>Id</th>
            <th>Full Name</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var s in Model)
        {
            <tr>
                <td>@s.PersonId</td>
                <td>@s.FullName</td>
            </tr>
        }
    </tbody>
</table>
